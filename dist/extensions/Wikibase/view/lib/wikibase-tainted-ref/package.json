{
  "name": "tainted-ref",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "WEBPACK_TARGET=dev vue-cli-service serve src/mock-entry.ts",
    "build": "npm-run-all build:*",
    "test:unit": "vue-cli-service test:unit -c ./jest.config.js",
    "build-storybook": "build-storybook",
    "build-to-temp": "npm-run-all \"build:* -- --dest $npm_package_config_tempBuildDirectory\" --",
    "build:app": "rm -rf node_modules/.cache && NODE_ENV=production vue-cli-service build --no-clean --target lib --formats commonjs src/main.ts",
    "build:init": "rm -rf node_modules/.cache && NODE_ENV=production vue-cli-service build --no-clean",
    "fix": "vue-cli-service lint --max-warnings 0 .",
    "prepare": "node build/wikimedia-ui-base.js",
    "storybook": "start-storybook",
    "test": "npm-run-all test:*",
    "test:lint": "vue-cli-service lint --max-warnings 0 --no-fix . && stylelint --syntax scss 'src/**/*.(vue|scss)'",
    "test:storybook": "CHOKIDAR_USEPOLLING=1 start-storybook --ci --smoke-test --quiet",
    "test:distnodiff": "npm run build-to-temp && build/testnodiff.sh dist/ $npm_package_config_tempBuildDirectory",
    "test:mwlibs": "echo 'disabled (T297381)' # ZUUL_BRANCH=${ZUUL_BRANCH:-master} lib-version-check",
    "watch": "vue-cli-service test:unit -c ./jest.config.js --watch"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "events": "^3.3.0",
    "vue": "3.2.23"
  },
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@storybook/addon-a11y": "^6.2.8",
    "@storybook/addon-actions": "^6.2.8",
    "@storybook/addon-docs": "^6.2.8",
    "@storybook/addon-links": "^6.2.8",
    "@storybook/addons": "^6.2.8",
    "@storybook/vue3": "^6.3.12",
    "@types/jest": "^27.0.2",
    "@types/jquery": "^3.5.5",
    "@types/node": "^12.20.7",
    "@types/uuid": "^3.4.9",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "@vue/cli-plugin-babel": "^4.5.12",
    "@vue/cli-plugin-eslint": "^4.5.12",
    "@vue/cli-plugin-typescript": "^4.5.12",
    "@vue/cli-plugin-unit-jest": "^5.0.0-rc.2",
    "@vue/cli-service": "^4.5.12",
    "@vue/eslint-config-typescript": "^7.0.0",
    "@vue/test-utils": "^2.0.0-rc.18",
    "@vue/vue3-jest": "^27.0.0-alpha.4",
    "@wmde/eslint-config-wikimedia-typescript": "^0.1.1",
    "@wmde/lib-version-check": "^0.1.0",
    "@wmde/vuex-helpers": "^0.1.2",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-preset-vue": "^2.0.2",
    "bootstrap": "^4.6.0",
    "eslint": "^6.7.2",
    "eslint-config-wikimedia": "^0.14.3",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-vue": "^6.2.2",
    "fork-ts-checker-webpack-plugin": "^1.6.0",
    "gonzales-pe": "^4.3.0",
    "http-status-codes": "^1.4.0",
    "less-variable-resolver": "^0.1.2",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "postcss-prefixwrap": "^1.22.1",
    "reset-css": "^4.0.1",
    "sass-loader": "^8.0.2",
    "sast": "^0.8.1",
    "stylelint": "^13.12.0",
    "stylelint-config-wikimedia": "^0.10.3",
    "stylelint-processor-html": "^1.0.0",
    "stylelint-scss": "^3.19.0",
    "ts-jest": "^27.0.7",
    "typescript": "~4.1.5",
    "url-search-params-polyfill": "^5.1.0",
    "vuex": "4.0.2",
    "wikimedia-ui-base": "^0.15.0"
  },
  "config": {
    "tempBuildDirectory": "/tmp/tainted-refs-build",
    "remoteVersion": {
      "vue": "https://raw.githubusercontent.com/wikimedia/mediawiki/#{ZUUL_BRANCH}/resources/lib/vue/vue.common.prod.js",
      "vuex": "https://raw.githubusercontent.com/wikimedia/mediawiki/#{ZUUL_BRANCH}/resources/lib/vuex/vuex.js"
    }
  }
}
