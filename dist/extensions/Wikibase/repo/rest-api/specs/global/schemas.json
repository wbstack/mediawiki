{
	"Item": {
		"type": "object",
		"properties": {
			"id": {
				"type": "string"
			},
			"type": {
				"type": "string"
			},
			"labels": {
				"$ref": "#/Labels"
			},
			"descriptions": {
				"$ref": "#/Descriptions"
			},
			"aliases": {
				"$ref": "#/Aliases"
			},
			"sitelinks": {
				"$ref": "./schema-parts.json#/Sitelinks"
			},
			"statements": {
				"$ref": "./schema-parts.json#/Statements"
			}
		},
		"discriminator": {
			"propertyName": "type"
		}
	},
	"Labels": {
		"type": "object",
		"additionalProperties": {
			"type": "string"
		},
		"example": {
			"en": "Douglas Adams",
			"fr": "Douglas Adams"
		}
	},
	"Descriptions": {
		"type": "object",
		"additionalProperties": {
			"type": "string"
		},
		"example": {
			"en": "Science-fiction writer",
			"fr": "écrivain de science-fiction"
		}
	},
	"Aliases": {
		"type": "object",
		"additionalProperties": {
			"type": "array",
			"items": { "type": "string" }
		},
		"example": {
			"en": [
				"Douglas Noel Adams",
				"Douglas Noël Adams"
			],
			"fr": [
				"Douglas Noel Adams"
			]
		}
	},
	"Sitelink": {
		"type": "object",
		"properties": {
			"site": {
				"type": "string"
			},
			"title": {
				"type": "string"
			},
			"badges": {
				"type": "array",
				"items": { "type": "string" }
			}
		}
	},
	"Statement": {
		"type": "object",
		"properties": {
			"id": {
				"type": "string",
				"description": "The globally unique identfier for this Statement",
				"readOnly": true
			},
			"mainsnak": {
				"$ref": "#/Snak"
			},
			"rank": {
				"type": "string",
				"default": "normal",
				"enum": [ "deprecated", "normal", "preferred" ]
			},
			"qualifiers": {
				"$ref": "./schema-parts.json#/Qualifiers"
			},
			"qualifiers-order": {
				"type": "array",
				"items": {
					"type": "string"
				}
			},
			"references": {
				"type": "array",
				"items": { "$ref": "#/Reference" }
			}
		}
	},
	"Reference": {
		"allOf": [
			{ "$ref": "./schema-parts.json#/HashBearing" },
			{
				"type": "object",
				"properties": {
					"hash": {
						"description": "Hash of a reference"
					},
					"snaks": {
						"$ref": "./schema-parts.json#/Snaks"
					},
					"snaks-order": {
						"type": "array",
						"items": {
							"type": "string"
						}
					}
				}
			}
		]
	},
	"Qualifier": {
		"allOf": [
			{ "$ref": "#/Snak" },
			{
				"properties": {
					"hash": {
						"description": "Hash of a qualifier"
					}
				}
			}
		]
	},
	"Snak": {
		"allOf": [
			{ "$ref": "./schema-parts.json#/HashBearing" },
			{
				"type": "object",
				"properties": {
					"snaktype": {
						"type": "string"
					},
					"property": {
						"type": "string"
					},
					"hash": {
						"description": "Hash of a snak"
					},
					"datatype": {
						"description": "The property data type",
						"type": "string",
						"readOnly": true
					},
					"datavalue": {
						"type": "object",
						"additionalProperties": true,
						"example": {
							"type": "string",
							"value": "I am a goat"
						}
					}
				}
			}
		]
	}
}
