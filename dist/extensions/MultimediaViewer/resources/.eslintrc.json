{
	"root": true,
	"extends": [
		"wikimedia/client-es5",
		"wikimedia/jquery",
		"wikimedia/mediawiki",
		"wikimedia/jsduck"
	],
	"rules": {
		"max-len": "off",
		"max-statements-per-line": "off",
		"no-jquery/no-class-state": "off",
		"no-jquery/no-support": "off",
		"no-shadow": "warn",
		"compat/compat": "warn"
	}
}
