{
	"extends": [
		"wikimedia/qunit",
		"wikimedia/language/es6"
	],
	"globals": {
		"sinon": false
	},
	"rules": {
		"compat/compat": [ "error", "last 2 chrome versions, last 2 firefox versions" ],
		"indent": "off",
		"no-jquery/no-html": "off",
		"no-jquery/no-parse-html-literal": "off"
	},
	"parserOptions": {
		"ecmaVersion": 2018
	}
}
