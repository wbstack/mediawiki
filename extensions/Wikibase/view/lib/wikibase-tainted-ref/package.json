{
  "name": "tainted-ref",
  "version": "0.1.0",
  "private": true,
  "config": {
    "tempBuildDirectory": "/tmp/tainted-refs-build",
    "remoteVersion": {
      "vue": "https://raw.githubusercontent.com/wikimedia/mediawiki/#{ZUUL_BRANCH}/resources/lib/vue/vue.common.prod.js",
      "vuex": "https://raw.githubusercontent.com/wikimedia/mediawiki/#{ZUUL_BRANCH}/resources/lib/vuex/vuex.js"
    }
  },
  "scripts": {
    "serve": "WEBPACK_TARGET=dev vue-cli-service serve src/mock-entry.ts",
    "prepare": "node build/wikimedia-ui-base.js",
    "build": "npm-run-all build:*",
    "build-to-temp": "npm-run-all \"build:* -- --dest $npm_package_config_tempBuildDirectory\" --",
    "build:app": "rm -rf node_modules/.cache && NODE_ENV=production vue-cli-service build --no-clean --target lib --formats commonjs src/main.ts",
    "build:init": "rm -rf node_modules/.cache && NODE_ENV=production vue-cli-service build --no-clean",
    "fix": "vue-cli-service lint --max-warnings 0 .",
    "watch": "vue-cli-service test:unit -c ./jest.config.js --watch",
    "test": "npm-run-all test:*",
    "test:unit": "vue-cli-service test:unit -c ./jest.config.js",
    "test:lint": "vue-cli-service lint --max-warnings 0 --no-fix . && stylelint --syntax scss 'src/**/*.(vue|scss)'",
    "test:distnodiff": "npm run build-to-temp && build/testnodiff.sh dist/ $npm_package_config_tempBuildDirectory",
    "test:mwlibs": "ZUUL_BRANCH=${ZUUL_BRANCH:-master} lib-version-check",
    "storybook": "start-storybook",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "core-js": "^2.6.11",
    "events": "^3.1.0",
    "vue": "^2.6.11",
    "vue-class-component": "^6.0.0",
    "vue-property-decorator": "^7.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@storybook/addon-a11y": "^5.3.19",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-links": "^5.3.19",
    "@storybook/addons": "^5.3.19",
    "@storybook/vue": "^5.3.19",
    "@types/jest": "^23.1.4",
    "@types/jquery": "^3.5.0",
    "@types/node": "^12.12.50",
    "@types/uuid": "^3.4.9",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "@vue/cli-plugin-babel": "^3.12.1",
    "@vue/cli-plugin-eslint": "^3.12.1",
    "@vue/cli-plugin-typescript": "^3.12.1",
    "@vue/cli-plugin-unit-jest": "^3.12.1",
    "@vue/cli-service": "^3.12.1",
    "@vue/eslint-config-typescript": "^4.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "@wdio/cli": "^5.23.0",
    "@wdio/local-runner": "^5.23.0",
    "@wdio/mocha-framework": "^5.23.0",
    "@wdio/spec-reporter": "^5.23.0",
    "@wdio/sync": "^5.23.0",
    "@wmde/eslint-config-wikimedia-typescript": "^0.1.1",
    "@wmde/lib-version-check": "^0.1.0",
    "@wmde/vuex-helpers": "^0.1.2",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.6",
    "babel-preset-vue": "^2.0.2",
    "bootstrap": "^4.5.0",
    "eslint": "^5.16.0",
    "eslint-config-wikimedia": "^0.14.3",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-vue": "^5.2.3",
    "eslint-plugin-wdio": "^5.13.2",
    "fork-ts-checker-webpack-plugin": "^1.6.0",
    "gonzales-pe": "^4.3.0",
    "http-status-codes": "^1.4.0",
    "less-variable-resolver": "^0.1.2",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "postcss-prefixwrap": "^1.16.0",
    "reset-css": "^4.0.1",
    "sass-loader": "^7.3.1",
    "sast": "^0.8.1",
    "storybook-addon-vue-info": "^1.4.2",
    "stylelint": "^13.6.1",
    "stylelint-config-wikimedia": "^0.10.1",
    "stylelint-processor-html": "^1.0.0",
    "stylelint-scss": "^3.18.0",
    "ts-jest": "^23.10.5",
    "typescript": "3.5.3",
    "url-search-params-polyfill": "^5.1.0",
    "vue-template-compiler": "^2.6.11",
    "vuex": "3.1.3",
    "vuex-class": "^0.3.2",
    "wdio-mediawiki": "0.4.0",
    "wdio-wikibase": "^0.3.0",
    "webdriverio": "^5.23.0",
    "wikimedia-ui-base": "^0.15.0"
  }
}
