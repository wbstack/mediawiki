{
  "name": "data-bridge",
  "version": "0.1.0",
  "private": true,
  "config": {
    "remoteVersion": {
      "vue": "https://raw.githubusercontent.com/wikimedia/mediawiki/#{ZUUL_BRANCH}/resources/lib/vue/vue.common.prod.js",
      "vuex": "https://raw.githubusercontent.com/wikimedia/mediawiki/#{ZUUL_BRANCH}/resources/lib/vuex/vuex.js"
    }
  },
  "scripts": {
    "serve": "WEBPACK_TARGET=dev vue-cli-service serve src/dev-entry.ts --skip-plugins eslint",
    "prepare": "node build/wikimedia-ui-base.js",
    "build": "NODE_ENV=production vue-cli-service build --no-clean --skip-plugins eslint --report --report-json --modern",
    "fix": "vue-cli-service lint --max-warnings 0 .",
    "test": "npm-run-all test:*",
    "test:lint": "vue-cli-service lint --max-warnings 0 --no-fix . && stylelint --syntax scss 'src/**/*.(vue|scss)'",
    "test:unit": "vue-cli-service test:unit -c ./jest.config.unit.js",
    "test:integration": "vue-cli-service test:unit -c ./jest.config.integration.js",
    "test:e2e": "vue-cli-service test:unit -c ./jest.config.e2e.js",
    "test:distnodiff": "npm run build -- --dest /tmp/dist && diff -q ./dist/data-bridge.app.js /tmp/dist/data-bridge.app.js && diff -q ./dist/data-bridge.app.modern.js /tmp/dist/data-bridge.app.modern.js && diff -q ./dist/css/data-bridge.app.css /tmp/dist/css/data-bridge.app.css && diff -q ./dist/data-bridge.init.js /tmp/dist/data-bridge.init.js && diff -q ./dist/vendor-chunks.js /tmp/dist/vendor-chunks.js",
    "test:storybook": "CHOKIDAR_USEPOLLING=1 start-storybook --ci --smoke-test --quiet",
    "test:mwlibs": "ZUUL_BRANCH=${ZUUL_BRANCH:-master} lib-version-check",
    "test-unit-coverage": "vue-cli-service test:unit -c ./jest.config.unit.js --coverage && sed -i 's|SF:/src/client/data-bridge/|SF:|g' coverage/lcov.info",
    "selenium-test": "wdio tests/selenium/wdio.conf.js",
    "storybook": "start-storybook",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "deep-equal": "^2.0.3",
    "events": "^3.2.0",
    "normalize.css": "^8.0.1",
    "serialize-error": "^7.0.1",
    "uuid": "^8.3.0",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.5",
    "vue-property-decorator": "^8.5.1",
    "vuex-smart-module": "^0.3.4"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@storybook/addon-a11y": "^6.0.28",
    "@storybook/addon-actions": "^6.1.21",
    "@storybook/addon-controls": "^6.1.21",
    "@storybook/addon-docs": "^6.1.21",
    "@storybook/addon-links": "^6.1.21",
    "@storybook/addons": "^6.1.21",
    "@storybook/vue": "^6.1.21",
    "@types/deep-equal": "^1.0.1",
    "@types/jest": "^24.9.1",
    "@types/jquery": "^3.5.1",
    "@types/node": "^12.12.58",
    "@types/uuid": "^3.4.9",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "@vue/cli-plugin-babel": "^4.5.6",
    "@vue/cli-plugin-eslint": "^4.5.6",
    "@vue/cli-plugin-typescript": "^4.5.6",
    "@vue/cli-plugin-unit-jest": "^4.5.6",
    "@vue/cli-service": "^4.5.6",
    "@vue/eslint-config-typescript": "^5.1.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "@wdio/cli": "^5.23.0",
    "@wdio/local-runner": "^5.23.0",
    "@wdio/mocha-framework": "^5.23.0",
    "@wdio/spec-reporter": "^5.23.0",
    "@wdio/sync": "^5.23.0",
    "@wmde/eslint-config-wikimedia-typescript": "^0.1.1",
    "@wmde/lib-version-check": "^0.1.0",
    "@wmde/vuex-helpers": "^0.1.2",
    "@wmde/wikibase-datamodel-types": "^0.1.0",
    "@wmde/wikibase-vuejs-components": "^0.1.9",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.6",
    "babel-preset-vue": "^2.0.2",
    "bootstrap": "^4.5.2",
    "cssjanus": "^1.3.2",
    "eslint": "^6.8.0",
    "eslint-config-wikimedia": "^0.15.3",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-jest-formatting": "^1.2.0",
    "eslint-plugin-vue": "^6.2.2",
    "eslint-plugin-wdio": "^5.13.2",
    "gonzales-pe": "^4.3.0",
    "http-status-codes": "^1.4.0",
    "jquery": "^3.5.1",
    "less-variable-resolver": "^0.1.2",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "oojs-ui": "^0.39.3",
    "postcss-prefixwrap": "^1.17.0",
    "sass-loader": "^8.0.2",
    "sast": "^0.8.1",
    "stylelint": "^13.7.1",
    "stylelint-config-wikimedia": "^0.10.1",
    "stylelint-processor-html": "^1.0.0",
    "stylelint-scss": "^3.18.0",
    "terser-webpack-plugin": "^2.3.8",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.6",
    "url-search-params-polyfill": "^8.1.0",
    "vue-eslint-parser": "^7.1.0",
    "vue-template-compiler": "^2.6.12",
    "vuex": "3.1.3",
    "wdio-mediawiki": "1.0.0",
    "wdio-wikibase": "^3.0.1",
    "wikimedia-ui-base": "^0.15.0"
  }
}
