{
	"name": "wikibase/changes",
	"description": "Library to manage changes to Wikibase Entities",
	"type": "library",
	"license": "GPL-2.0-or-later",
	"require": {
		"diff/diff": "^3.2.0",
		"psr/log": "^1.1.3",
		"wikibase/data-model": "^9.5.1",
		"wikibase/data-model-services": "~5.1",
		"wikimedia/assert": "^0.5.0",
		"wikimedia/at-ease": "^2.0.0",
		"wikimedia/timestamp": "^3.0.0"
	},
	"require-dev": {
		"phpunit/phpunit": "^8.5",
		"wikimedia/testing-access-wrapper": "~1.0"
	},
	"autoload": {
		"psr-4": {
			"Wikibase\\Lib\\Changes\\": "src/",
			"Wikibase\\Lib\\Tests\\Changes\\": "tests/"
		}
	},
	"scripts": {
		"test": [
			"phpunit tests/"
		]
	}
}
